			PopupPanelLocation="Center"
			ShowOnPageLoad="False"
			Width="500"
			WidthScale="px"
			>
			<Division
				Class="panel panel-default"
				HtmlDiv="True"
				ID="divCommView"
				>
				<Label
					HtmlTag="p"
					ID="lblZoomComments"
				/>
			</Division>
		</PopupPanel>
